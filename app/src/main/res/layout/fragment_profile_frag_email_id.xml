<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".profileFragEmailId">

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="689dp"
        android:clipToPadding="true"
        app:cardBackgroundColor="#69C5B3"
        app:cardCornerRadius="20dp"
        app:cardElevation="20dp"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintYo"
            android:layout_width="wrap_content"
            android:layout_height="683dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageShow"
                android:layout_width="164dp"
                android:layout_height="156dp"
                android:layout_marginStart="109dp"
                android:layout_marginLeft="109dp"
                android:layout_marginTop="44dp"
                android:layout_marginEnd="109dp"
                android:layout_marginRight="109dp"
                android:adjustViewBounds="false"
                android:cropToPadding="false"
                android:src="@drawable/profile"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/nameID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="120dp"
                android:layout_marginRight="120dp"
                android:layout_weight="1"
                android:fontFamily="@font/berkshire_swash"
                android:gravity="center"
                android:text="name"
                android:textSize="34sp"
                app:layout_constraintEnd_toEndOf="@id/constraintYo"
                app:layout_constraintStart_toStartOf="@id/constraintYo"
                app:layout_constraintTop_toBottomOf="@+id/imageShow" />

            <TextView
                android:id="@+id/emailID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="320dp"
                android:layout_weight="1"
                android:fontFamily="@font/advent_pro_medium"
                android:textSize="34sp"
                app:layout_constraintEnd_toEndOf="@id/constraintYo"
                app:layout_constraintHorizontal_bias="0.187"
                app:layout_constraintStart_toStartOf="@id/constraintYo"
                app:layout_constraintTop_toTopOf="@id/constraintYo" />

            <TextView
                android:id="@+id/phoneID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="148dp"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="316dp"
                android:layout_marginEnd="172dp"
                android:layout_marginRight="172dp"
                android:fontFamily="@font/berkshire_swash"
                android:textSize="34sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/constraintYo" />

            <Button
                android:id="@+id/viewProfileEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="44dp"
                android:layout_marginLeft="44dp"
                android:layout_marginTop="126dp"
                android:layout_marginEnd="44dp"
                android:layout_marginRight="44dp"
                android:fontFamily="@font/amaranth_italic"
                android:text="View or edit your profile"
                app:backgroundTint="#E15586"
                app:cornerRadius="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameID" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="278dp"
                android:layout_height="1dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:background="#FD0059"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameID" />

            <TextView
                android:id="@+id/textViewLine"
                android:layout_width="278dp"
                android:layout_height="1dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="380dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:background="#FF0059"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@id/constraintYo"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


</FrameLayout>
